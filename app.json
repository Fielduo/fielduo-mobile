{
  "expo": {
    "name": "Fielduo",
    "slug": "fielduo-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon_v2.png",
    "scheme": "fielduomobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Spalsh.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.santhiyak.fielduomobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Voice to text conversion",
        "NSSpeechRecognitionUsageDescription": "Voice to text conversion"
      }
    },
    "android": {
      "package": "com.santhiyak.fielduomobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_v2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-speech-recognition"
      
    ],
    "extra": {
      "MAPBOX_ACCESS_TOKEN": "pk.eyJ1Ijoia2lydXRoaWdhbmUiLCJhIjoiY21oYXJyeXdqMWozZjJsczV5dDlycnd5NyJ9.Bk5zF4AQxBeWBJjHux3UHQ",
      "eas": {
        "projectId": "b56e2ae2-0117-43ef-9e4b-11cf4d4c96d2"
      }
    }
  }
}
