{
  "expo": {
    "name": "fielduo-mobile",
    "slug": "fielduo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fielduomobile",
    "userInterfaceStyle": "automatic",
  "splash": {
  "image": "./assets/images/Spalsh.png",
  "resizeMode": "contain",
  "backgroundColor": "#ffffff"
},

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.santhiyak.fielduomobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.santhiyak.fielduomobile",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-sqlite"],
    "extra": {
      "MAPBOX_ACCESS_TOKEN": "pk.eyJ1Ijoia2lydXRoaWdhbmUiLCJhIjoiY21oYXJyeXdqMWozZjJsczV5dDlycnd5NyJ9.Bk5zF4AQxBeWBJjHux3UHQ",
      "eas": {
        "projectId": "ae17c76e-4467-4ebe-91a6-a0f006ff0a7d"
      }
    }
    
  }
}
